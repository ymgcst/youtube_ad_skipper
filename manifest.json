{
  "manifest_version": 3,
  "name": "YouTube Ad Skipper",
  "version": "1.0",
  "description": "Automatically click the 'Skip Ad' button on YouTube.",
  "icons": { "128": "icon.png" },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab"]
}
